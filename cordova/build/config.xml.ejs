<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="<%= bundleVersion %>" id="<%= packageName %>" ios-CFBundleVersion="<%= bundleVersion %>" version="<%= version %>" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name><%= appName %></name>
  <description><%= appDescription %></description>
  <author email="<%= authorEmail %>" href="<%= authorWebsite %>"><%= authorName %></author>
  <content src="index.html" />
  <access origin="*" />
  <preference name="BackupWebStorage" value="none" />
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="target-device" value="handset"/>
  <preference name="KeyboardDisplayRequiresUserAction" value="false"/>
  <preference name="android-minSdkVersion" value="21"/>
  <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine"/>
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <feature name="CDVWKWebViewEngine">
    <param name="ios-package" value="CDVWKWebViewEngine" />
  </feature>
  <platform name="android">
    <allow-intent href="market:*" />
    <%- androidIcons.join('\n    ') %>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <%- iosIcons.join('\n    ') %>
  </platform>
</widget>
